{
  "window_name": "Genshin Twitch Wish Simulator",
  "language": "russian",
  "messages": "messages_russian",
  "html_template": "history_template_russian.html",
  "banner": "all_in_one",
  "chat_bot": {
    "enabled": true,
    "wish_command": "wish",
    "wish_command_prefix": "!",
    "wish_global_timeout": 1,
    "wish_timeout": {
      "broadcaster": 600,
      "mod": 600,
      "vip": 600,
      "turbo": 600,
      "subscriber": 600,
      "user": 600
    },
    "send_notify": false,
    "wish_count": 1,
    "self_wish": false,
    "self_wish_every": 60,
    "enable_colors": true
  },
  "event_bot": {
    "enabled": true,
    "default_color": "#FFFFFF",
    "rewards": [
      {
        "event_name": "Одна крутка",
        "wish_count": 1
      },
      {
        "event_name": "Много круток",
        "wish_count": 5
      },
      {
        "event_name": "Очень много круток",
        "wish_count": 10
      }
    ]
  },
  "animations": {
    "chroma_color": "#00FF00",
    "draw_states": {
      "draw_usertext": true,
      "draw_fall": true,
      "draw_wishes": true
    },
    "start_delay": 4,
    "end_delay": {
      "3": 6,
      "4": 10,
      "5": 14
    },
    "end_delay_milti": {
      "3": 1,
      "4": 7,
      "5": 14
    },
    "user_background": {
      "enabled": false,
      "path": "",
      "type": "static"
    },
    "font": {
      "path": "Genshin_Impact.ttf",
      "user_uid_size": 48,
      "wish_name_size": 42
    },
    "fps": 30
  },
  "sound": {
    "enabled": true,
    "fall": "soundfall.mp3",
    "3": "3star.mp3",
    "4": "4star.mp3",
    "5": "5star.mp3"
  },
  "history_file": {
    "enabled": true,
    "path": "users_wish_history.csv",
    "3": true,
    "4": true,
    "5": true
  },
  "gbot_config": {
    "gbot_status": {
      "enabled": true,
      "timeout": 30,
      "permissions": {
        "broadcaster": true,
        "mod": true,
        "vip": true,
        "turbo": true,
        "subscriber": true,
        "user": true
      }
    },
    "gbot_stats": {
      "enabled": true,
      "timeout": 5,
      "permissions": {
        "broadcaster": true,
        "mod": true,
        "vip": true,
        "turbo": true,
        "subscriber": true,
        "user": true
      }
    },
    "gbot_sound": {
      "enabled": true,
      "timeout": 1,
      "permissions": {
        "broadcaster": true,
        "mod": true,
        "vip": false,
        "turbo": false,
        "subscriber": false,
        "user": false
      }
    },
    "gbot_pause": {
      "enabled": true,
      "timeout": 1,
      "permissions": {
        "broadcaster": true,
        "mod": true,
        "vip": false,
        "turbo": false,
        "subscriber": false,
        "user": false
      }
    },
    "gbot_history": {
      "enabled": true,
      "timeout": 5,
      "permissions": {
        "broadcaster": true,
        "mod": true,
        "vip": true,
        "turbo": true,
        "subscriber": true,
        "user": true
      }
    },
    "gbot_history_all": {
      "enabled": true,
      "timeout": 1,
      "permissions": {
        "broadcaster": true,
        "mod": true,
        "vip": false,
        "turbo": false,
        "subscriber": false,
        "user": false
      }
    }
  },
  "send_dev_stats": true,
  "test_mode": true
}